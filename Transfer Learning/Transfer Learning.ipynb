   "source": [
    "vgg19_model.layers.pop()\n",
    "new_layer = Dense(2, activation='softmax', name='predictions')\n",
    "inp = vgg19_model.input\n",
    "out = new_layer(vgg19_model.layers[-1].output)\n",
